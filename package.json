{
  "name": "@ryanwalters/jwplayer-react",
  "version": "0.0.0",
  "description": "A react component that creates an instance of JW Player",
  "license": "MIT",
  "author": "Ryan Walters",
  "homepage": "https://github.com/ryanwalters/jwplayer-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanwalters/jwplayer-react.git"
  },
  "bugs": {
    "url": "https://github.com/ryanwalters/jwplayer-react/issues"
  },
  "main": "./lib/jwplayer-react.js",
  "types": "./lib/jwplayer-react.d.ts",
  "files": [
    "lib/jwplayer-react.d.ts",
    "lib/jwplayer-react.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "ci": "npm ci; npm run build; npm run test",
    "prepare": "husky",
    "publish": "semantic-release",
    "publish:dry": "semantic-release --dry-run",
    "test": "vitest run --coverage"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@testing-library/react": "16.3.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "@vitejs/plugin-react": "5.1.0",
    "@vitest/coverage-v8": "4.0.7",
    "@vitest/ui": "4.0.7",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "husky": "9.1.7",
    "jsdom": "27.1.0",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.3.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "semantic-release": "25.0.1",
    "tsup": "8.5.0",
    "typescript": "5.9.3",
    "vitest": "4.0.7"
  },
  "keywords": [
    "jw",
    "jwplayer",
    "player",
    "react"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,scss,md}": "prettier --write"
  }
}
